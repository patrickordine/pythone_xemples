def bubbleSort(array):
    n = len(array)

    for i in range(n):
       
        for j in range(0, n-i-1):
            
            if array[j] > array[j+1] :
                
                array[j], array[j+1] = array[j+1], array[j]


array = [64, 323, 2, 44, 35, 66, 1080, 34, 25, 12, 22, 11, 90]
print("Array desordenado:", array)

bubbleSort(array)

print("Array ordenado:", array)
